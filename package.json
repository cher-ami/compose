    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.19",
    "gsap": "^3.3.4",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,less,css,json,md}": [
      "prettier --write"
    ]
