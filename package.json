{
  "name": "@wbe/composition",
  "version": "2.3.1",
  "description": "Composition is a tiny zero dependency library for vanilla javascript component approach and page transitions management.",
  "author": "Willy Brauner",
  "license": "MIT",
  "types": "dist/index",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "private": false,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/willybrauner/libraries.git"
