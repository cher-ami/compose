{
  "name": "@wbe/composition",
  "version": "0.1.0",
  "description": "A DOM composition API",
  "author": "Willy Brauner",
  "license": "MIT",
  "main": "dist/index.js",
  "private": false,
  "keywords": [
    "router",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/willybrauner/composition.git"
  },
